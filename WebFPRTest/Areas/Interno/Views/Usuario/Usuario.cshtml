@model WebFPRTest.Areas.Interno.Models.Usuario.UsuarioViewModel
@{
    Layout = "_LayoutInterno";
}
<form>
    <div class="container">
        <div class="row mt-2 g-2">
            <div class="col-12 col-sm-12 col-md-6 col-lg-3">
                <div class="form-floating">
                    <input type="text" id="Paterno" class="form-control" asp-for="Paterno" />
                    <label for="Paterno">Paterno</label>
                </div>
            </div>
            <div class="col-12 col-sm-12 col-md-6 col-lg-3">
                <div class="form-floating">
                    <input type="text" id="Materno" class="form-control" asp-for="Materno" />
                    <label for="Materno">Materno</label>
                </div>
            </div>
            <div class="col-12 col-sm-12 col-md-6 col-lg-6">
                <div class="form-floating">
                    <input type="text" id="Nombres" class="form-control" asp-for="Nombres" />
                    <label for="Nombres">Nombres</label>
                </div>
            </div>
            <div class="col-12 col-sm-12 col-md-6 col-lg-3">
               <div class="form-floating">
                    <select class="form-select" asp-for="Id_011_TipoUsuario" id="Id_011_TipoUsuario">
                       <option></option>
                        @foreach (var tipoUsuario in Model.TipoUsuarioList)
                        {
                            <option value="@tipoUsuario.Id_Parametro">@tipoUsuario.Descripcion</option>
                        }
                    </select>
                   <label></label>
               </div>
               
            </div>
            <div class="col-12 col-sm-12 col-md-6 col-lg-3">
                <div class="form-floating">
                    <input type="text" id="Documento" class="form-control" asp-for="Documento"/>
                    <label for="Documento">N° Documento</label>
                </div>
            </div>
            <div class="col-12 col-sm-12 col-md-6 col-lg-6">
                <div class="form-floating">
                    <input type="text" id="Correo" class="form-control" asp-for="Correo" />
                    <label for="Correo">N° Correo</label>
                </div>
            </div>
            <div class="col-12 col-sm-12 col-md-6 col-lg-3">
                <div class="form-floating">
                    <select class="form-select" asp-for="Id_001_TipoDocumento" id="Id_001_TipoDocumento">
                        <option></option>
                        @foreach (var tipoDocumento in Model.TipoDocumentoList)
                        {
                            <option value="@tipoDocumento.Id_Parametro">@tipoDocumento.Abreviatura</option>
                        }
                    </select>
                    <label></label>
                </div>
            </div>
            <div class="col-12 col-sm-12 col-md-6 col-lg-3">
                <div class="form-floating">
                    <input type="text" id="Usuario" class="form-control" asp-for="Usuario" />
                    <label for="Usuario">Usuario</label>
                </div>
            </div>
            <div class="col-12 col-sm-12 col-md-6 col-lg-3">
                <div class="form-floating">
                    <input type="text" id="Clave" class="form-control" asp-for="Clave" />
                    <label for="Clave">Clave</label>
                </div>
            </div>
            <div class="col-12 col-sm-12 col-md-6 col-lg-3">
                <div class="form-floating">
                    <input type="text" id="ClaveConfirmacion" class="form-control" asp-for="ClaveConfirmacion" />
                    <label for="ClaveConfirmacion">Confirmar Clave</label>
                </div>
            </div>
        </div>
        <div class="mt-2">
            <button class="btn btn-outline-success">Registrar</button>
            <button class="btn btn-outline-danger">Anular</button>
        </div>
    </div>
</form>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
<script src="//code.jquery.com/jquery-1.11.0.min.js"></script>

